{
  "name": "test-ts-doc",
  "version": "1.0.0",
  "description": "test documentation generation",
  "source": "src/index.ts",
  "main": "dist/ts-doc.umd.cjs",
  "module": "dist/ts-doc.umd.cjs",
  "types": "dist/types.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
