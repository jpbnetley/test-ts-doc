    "test": "echo \"Error: no test specified\" && exit 1",
    "serve:docs": "vite function-docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpbnetley/test-ts-doc.git"
  },
  "keywords": [],
  "author": "",
